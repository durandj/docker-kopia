# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[Dockerfile]
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 2

[*.{yml,yaml}]
indent_style = space
indent_size = 2

[.vscode/*.json]
indent_style = space
indent_size = 4
